<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.DemoMapper">

	<select id="list" resultType="com.demo.pojo.Demo">
		select * from demo
	</select>

	<select id="getById" resultType="com.demo.pojo.Demo">
		select * from demo where id = #{id}
	</select>

	<insert id="save" parameterType="com.demo.pojo.Demo">
		insert into demo(name, age) values (#{name}, #{age})
	</insert>
</mapper>
